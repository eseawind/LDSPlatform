        "documentLineId": "CDFG8400-E29B-11D4-A716-446655440000",
        "documentHeaderId": "550E8400-E29B-11D4-A716-446655440000",
        "level1": 1,
        "level2": 0,
        "level3": 0,
        "resultCode": "OK",
        "resultText": "92 34 57 88 90",
